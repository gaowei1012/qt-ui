{
  "name": "qt-ui",
  "version": "1.0.0",
  "private": true,
  "description": "qt-ui",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test && jest --coverage",
