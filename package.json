{
  "name": "qt-ui",
  "version": "1.0.0",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "yarn run dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:rollup": "rollup --config ./config/rollup.config.js",
    "build:clean": "yarn run clean && yarn run build",
    "lint": "eslint ./src --fix",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:ci": "yarn run build:h5 && yarn run test",
    "test:analyze": "rollup --config ./config/rollup.config.analyze.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf .temp dist lib coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@tarojs/components": "~3.0.2",
    "@tarojs/react": "~3.0.2",
    "@tarojs/taro": "~3.0.2",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.11.1",
    "rollup": "^2.3.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-visualizer": "^4.0.2"
  },
  "dependencies": {
    "@antv/f2": "^3.8.10-beta.1",
    "@tarojs/components": "3.3.12",
    "@tarojs/react": "3.3.12",
    "@tarojs/runtime": "3.3.12",
    "@tarojs/taro": "3.3.12",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3",
    "@tarojs/react": ">=3",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  }
}
